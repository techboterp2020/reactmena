<?xml version="1.0" encoding="UTF-8" ?>
<odoo>

    <record id="paperformat_barcode_10_100_label" model="report.paperformat">
        <field name="name">Label</field>
        <field name="default" eval="True"/>
        <field name="format">custom</field>
        <field name="page_height">50</field>
        <field name="page_width">100</field>
        <field name="orientation">Portrait</field>
        <field name="margin_top">0</field>
        <field name="margin_bottom">0</field>
        <field name="margin_left">0</field>
        <field name="margin_right">0</field>
        <field name="disable_shrinking" eval="True"/>
        <field name="header_line" eval="False"/>
        <field name="header_spacing">0</field>
        <field name="dpi">96</field>
    </record>


    <report
            id="custom_barcode_label_print"
            model="product.label.layout"
            string="Product Barcode 50x100"
            report_type="qweb-pdf"
            name="techbot_barcode_customisation.custom_barcode_label_print_new"
            file="techbot_barcode_customisation.custom_barcode_label_print_new"
            paperformat="paperformat_barcode_10_100_label"
            attachment_use="False"
    />

    <report
            id="custom_barcode_lot_print_action"
            model="stock.lot"
            string="Lot/Serial No 50x100"
            report_type="qweb-pdf"
            name="techbot_barcode_customisation.product_barcode_lot_report"
            file="techbot_barcode_customisation.product_barcode_lot_report"
            paperformat="paperformat_barcode_10_100_label"
            attachment_use="False"
    />


</odoo>